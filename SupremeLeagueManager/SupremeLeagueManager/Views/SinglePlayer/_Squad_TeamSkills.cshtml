
<table class="table table-dark table-striped table-sm mt-1 squadTable">
    <thead>
        <tr>
            <th colspan="2"><img style="visibility:hidden" v-bind:src="TeamEmblem()" width="50" height="50" /></th>
        </tr>
    </thead>
    <tbody style="font-size:13px">
        <tr>
            <td>Goalkeeper</td>
            <td class="text-center">{{ Team.GoalKeeper }}</td>
        </tr>
        <tr>
            <td>Defence</td>
            <td class="text-center">{{ Team.Defence }}</td>
        </tr>
        <tr>
            <td>Midfield</td>
            <td class="text-center">{{ Team.Midfield }}</td>
        </tr>
        <tr>
            <td style="line-height: 24px;">Attack</td>
            <td class="text-center">{{ Team.Attack}}</td>
        </tr>
        <tr>
            <td>Speed</td>
            <td class="text-center">{{ Team.Speed }}</td>
        </tr>
        <tr>
            <td>Technique</td>
            <td class="text-center">{{ Team.Technique }}</td>
        </tr>
        <tr>
            <td>Overall</td>
            <td class="text-center text-danger"> {{ Team.Overall }}</td>
        </tr>
        <tr><td><div class="invisible">.</div></td><td></td></tr>
        <tr><td><div class="invisible">.</div></td><td></td></tr>
    </tbody>
</table>
<div class="pane ml-2 mt-2">
    <div class="player-photo">
        <img v-bind:src="GetPlayerPhoto()" alt="no photo avilable" />
    </div>
</div>
