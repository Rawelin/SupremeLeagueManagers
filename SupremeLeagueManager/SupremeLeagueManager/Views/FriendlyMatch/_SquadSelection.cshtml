                    </div>
                    <div class="p-1" style="width: 15em;">
                        <vue-slider ref="slider" v-model="SliderObject.Aggression" v-bind="SliderOption" style="padding-top: 0.6em; width: 95%;"></vue-slider>
                    </div>
                </div>
            </div>
        </div>
       
